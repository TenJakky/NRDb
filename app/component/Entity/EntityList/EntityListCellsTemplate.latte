{define col-original_title}
    <td>
        <a href="{plink "{$eType}:view" $row->id}">{$row->original_title}</a>
    </td>
{/define}

{define col-number}
    <td>
        <a href="{plink "Season:view" $row->id}">{$row->number}</a>
    </td>
{/define}

{define col-maker}
    <td>
        {artistList $row->related("jun_{$etype}2{$mtype}.{$etype}_id")}
    </td>
{/define}

{define col-year}
    <td class="center">
        {$row->year}
    </td>
{/define}

{define col-head-class-year}
    center
{/define}

{define col-rating}
    <td class="center">
        {$ratingModel->getRating($row->id)|number: 2}
    </td>
{/define}

{define col-head-class-rating}
    center
{/define}

{define col-men_rating}
    <td class="center">
        {$ratingModel->getMenRating($row->id)|number: 2}
    </td>
{/define}

{define col-head-class-men_rating}
    center
{/define}

{define col-women_rating}
    <td class="center">
        {$ratingModel->getWomenRating($row->id)|number: 2}
    </td>
{/define}

{define col-head-class-women_rating}
    center
{/define}

{define col-my_rating}
    <td class="center">
        {var $rating => $ratingModel->getUserRating($row->id, $userId)}
        {if $rating}
            {$rating->rating}
        {/if}
    </td>
{/define}

{define col-head-class-my_rating}
    center
{/define}

{define col-action}
    <td>
        {var $rating => $ratingModel->getUserRating($row->id, $userId)}
        {if $rating}
            <a href="{plink "{$eType}:editRating" $rating->id}">Edit rating</a>
        {else}
            <a href="{plink "{$eType}:rate" $row->id}">Rate</a>
        {/if}
    </td>
{/define}
